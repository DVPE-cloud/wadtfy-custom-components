{
  "$schema": "http://json-schema.org/draft-07/schema",
  "required": [
    "aws",
    "eks"
  ],
  "properties": {
    "aws": {
      "type": "object",
      "required": [
        "accountId",
        "cidrIp"
      ],
      "properties": {
        "accountId": {
          "type": "string"
          },
        "cidrIp": {
          "type": "string"
        }
      }
    },
    "eks": {
      "type": "object",
      "required": [
        "csiDriverControllerSaRole",
        "csiDriverNodeSaRole"
      ],
      "properties": {
        "csiDriverControllerSaRole": {
          "type": "string"
          },
        "csiDriverNodeSaRole": {
          "type": "string"
        }
      }
    }
  }
}
